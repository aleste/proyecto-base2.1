<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Proyecto Base{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets output='css/proyecto-base.css' filter='yui_css'
                  '../app/Resources/public/bootstrap/css/bootstrap.min.css'
                  '../app/Resources/public/bootstrap/css/bootstrap-responsive.min.css'
            %}
            <link rel="stylesheet" href="{{ asset_url }}" />     
            {% endstylesheets %} 
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            {% javascripts output='js/proyecto-base.js' filter='yui_js'
                '../app/Resources/public/jquery/jquery-1.8.3.min.js'            
                '../app/Resources/public/bootstrap/js/bootstrap.min.js'
                '../app/Resources/public/jquery/jquery.maskedinput-1.3.min.js'
                '../app/Resources/public/jquery/jquery.bestupper.min.js'
            %}
             <script src="{{ asset_url }}"></script>                        
             <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
             <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>             
             {% endjavascripts %}  
        {% endblock %}
    </body>
</html>
